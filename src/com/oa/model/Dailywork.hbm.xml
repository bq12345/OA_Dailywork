<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.oa.model">

	<class name="com.oa.model.Dailywork" table="dailywork">
		<id name="id">
			<generator class="increment"/>
		</id>
		<property name="date"/>
		<property name="start_time"/>
		<property name="end_time"/>
		<property name="repeat_time"/>
		<property name="topic"/>
		<property name="data"/>
		<property name="remind"/>
		<property name="standard" type="boolean" />
	</class>
</hibernate-mapping>