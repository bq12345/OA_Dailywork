<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.oa.model">

	<class name="com.oa.model.CardClientOwner" table="card_client_owner">
		<id name="id">
			<generator class="increment"/>
		</id>
		<property name="client_name"/>
		<property name="team"/>
		<property name="work"/>
		<property name="work_post"/>
		<property name="mobile_phone"/>
		<property name="email"/>
		<property name="work_phone"/>
		<property name="work_fax"/>
		<property name="work_address"/>
	</class>
</hibernate-mapping>